var connection = require('../config/database');

test('database should be connected and query must be working', done => {
	connection.connect();

	connection.query('SELECT DATABASE();', function (error, results, fields) {
		if (error) throw error;
		expect(results[0]['DATABASE()']).toBe('osam');
		done();
	});
	connection.end();
});

test('select from table user must be working', done => {
	connection.connect();

	connection.query('SELECT * FROM user;', function (error, results, fields) {
		if (error) throw error;
		console.log(results[0]);
		done();
	});
	connection.end();
});
