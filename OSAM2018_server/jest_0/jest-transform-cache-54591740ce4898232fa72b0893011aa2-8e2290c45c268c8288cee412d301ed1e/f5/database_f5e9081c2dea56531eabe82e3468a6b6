require('dotenv').config();
var mysql = require('mysql');

module.export = {
  init: function () {
    return mysql.createConnection({
      host: process.env.DB_HOST,
      user: process.env.DB_USER,
      password: process.env.DB_PW,
      database: process.env.DB_NAME
    });
  }
};
