var connection = require('../config/database');

test('database should be connected and query must be working', done => {
	connection.connect();

	connection.query('SELECT DATABASE();', function (error, results, fields) {
		if (error) throw error;
		console.log(results[0]);
		done();
	});

	connection.end();
});
